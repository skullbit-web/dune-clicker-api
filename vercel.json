{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/leaderboard",
      "dest": "/api/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/submit",
      "dest": "/api/index.js",
      "methods": ["POST"]
    }
  ]
}
